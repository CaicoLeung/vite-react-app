{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "paths": {
      "@/*": ["src/*"],
      "#COMP/*": ["src/components/*"],
      "#LIBS/*": ["src/libs/*"],
      "#LIBS": ["src/libs/index"],
      "#UTILS/*": ["src/utils/*"],
      "#UTILS": ["src/utils/index"]
    }
  },
  "include": ["./src"]
}